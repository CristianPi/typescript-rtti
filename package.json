{
  "name": "typescript-rtti",
  "version": "0.0.1",
  "description": "Emits Typescript metadata for use at runtime",
  "main": "dist/index.js",
  "module": "dist.esm/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/rezonant/typescript-rtti",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rezonant/typescript-rtti.git"
  },
  "bugs": {
    "url": "https://github.com/rezonant/typescript-rtti/issues"
  },
  "keywords": [
    "rtti",
    "metadata",
    "decorators",
    "typescript",
    "runtime",
    "reflection",
    "reflect"
  ],
  "scripts": {
    "clean": "rimraf dist dist.esm",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "test": "rimraf dist && npm run build:cjs && node dist/test"
  },
  "author": "William Lahti",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "chai": "^4.3.0",
    "razmin": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.2"
  }
}
